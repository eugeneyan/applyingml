{
  "name": "applied-ml-site",
  "private": true,
  "description": "A site for applied-ml",
  "version": "0.1.0",
  "homepage": "https://eugeneyan.github.io/applied-ml-repo",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
